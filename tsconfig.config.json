{
  "extends": "@vue/tsconfig/tsconfig.node.json",
  "include": [
    "package.json",
    "vite.config.*",
    "vitest.config.*",
    "cypress.config.*",
    "postcss.config.*",
    "tailwind.config.*",
    "build/*.ts",
    "build/*.d.ts",
    "build/**/*.ts",
    "build/**/*.d.ts",
    "src/types/*.d.ts",
  ],
  "compilerOptions": {
    "composite": true,
    "types": [
      "node",
      "naive-ui/volar"
    ],
  }
}