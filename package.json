    "@vue/tsconfig": "^0.1.3",
    "naive-ui": "^2.32.2",
    "npm-run-all": "^4.1.5",
    "sass": "^1.54.5",
    "typescript": "~4.7.4",
    "unplugin-auto-import": "^0.11.2",
    "unplugin-vue-components": "^0.22.4",
    "vite": "^3.0.4",
    "vue-tsc": "^0.39.5"
  }
